<Page x:Class="Project.Storage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1280"
      Title="Storage">

    <Grid>
        <DataGrid Name="dgrid" HorizontalAlignment="Center" Height="359" Margin="310,291,310,70" VerticalAlignment="Center" Width="660" RenderTransformOrigin="0.5,0.5" >
            <DataGrid.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.077"/>
                    <TranslateTransform/>
                </TransformGroup>
            </DataGrid.RenderTransform>
        </DataGrid>
        <Button Content="Back" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="81" Height="26" Click="Button_Click_1" Background="#FF2749E2" Foreground="White"/>
        <TextBox Name="textb1" HorizontalAlignment="Center" Height="23" Margin="310,262,310,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="660" TextChanged="TextBox_TextChanged"/>
        <Label Content="Food Storage" HorizontalAlignment="Center" Margin="0,109,0,0" VerticalAlignment="Top" Width="193" FontSize="30"/>
    </Grid>
</Page>
